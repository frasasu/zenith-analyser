                VISION[vision]:"Build computer vision project"
            GROUP:(NEURAL 25.0^15.0 - DEEP 25.0^15.0 - NLP 20.0^10.0 - VISION 20.0^0)
        end_law
    end_target
end_target

# Event with Multiple Occurrences
law recurring_events:
    start_date:2024-01-15 at 09:00
